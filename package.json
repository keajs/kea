{
  "name": "kea",
  "version": "0.27.3",
  "description": "Smart front-end architecture",
  "author": "Marius Andra",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keajs/kea.git"
  },
  "bugs": {
    "url": "https://github.com/keajs/kea/issues"
  },
  "homepage": "https://github.com/keajs/kea#readme",
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "15 KB"
    }
  ],
  "scripts": {
    "build": "npm run lint && concurrently \"npm run build:cjs\" \"npm run build:es\"",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "start": "concurrently --kill-others \"npm run start:cjs\" \"npm run start:es\"",
    "start:cjs": "cross-env BABEL_ENV=cjs babel -w src --out-dir lib",
    "start:es": "cross-env BABEL_ENV=es babel -w src --out-dir es",
    "size": "size-limit",
    "test": "jest",
    "lint": "eslint src/**",
    "prepublish": "npm run build",
    "postinstall": "opencollective postinstall"
  },
  "files": [
    "es",
    "lib",
    "src",
    "saga.js"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/__tests__/helper/"
    ]
  },
  "dependencies": {
    "opencollective": "^1.0.3"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-redux": "5.x",
    "redux": "3.x",
    "reselect": "2.x || 3.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.0.2",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^11.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "size-limit": "^0.11.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/kea",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  }
}
