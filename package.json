{
  "name": "kea",
  "version": "1.0.0-rc.2",
  "description": "Smart front-end architecture",
  "author": "Marius Andra",
  "license": "MIT",
  "main": "lib/index.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keajs/kea.git"
  },
  "bugs": {
    "url": "https://github.com/keajs/kea/issues"
  },
  "homepage": "https://github.com/keajs/kea#readme",
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "15 KB"
    }
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "lint": "eslint src/**",
    "prepublish": "npm run test && npm run build",
    "start": "npm run watch",
    "test": "BABEL_ENV=test jest"
  },
  "files": [
    "es",
    "lib",
    "src",
    "saga.js"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/node_modules/",
      "<rootDir>/src/__tests__/helper/"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "opencollective": "^1.0.3"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-redux": ">= 7",
    "redux": ">= 3",
    "reselect": ">= 2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-babel-env-inline": "^0.0.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^15.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "reselect": "^4.0.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "size-limit": "^1.1.2"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/kea",
    "logo": "https://opencollective.com/opencollective/logo.txt"
